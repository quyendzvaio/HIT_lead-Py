coded_str = input("Nhập vào chuỗi mã hóa: ")

stack = []          
current_str = ""     
num = 0             

for ch in coded_str:
    if ch.isdigit():
        num = int(ch)             
    elif ch == '[':
        stack.append((current_str, num))
        current_str = ""
        num = 0
    elif ch == ']':
        last_str, repeat = stack.pop()
        current_str = last_str + current_str * repeat
    else:
        current_str += ch      
print("➡️ Chuỗi sau khi giải mã là:", current_str)
